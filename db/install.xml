<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/s3coursedelete/db" VERSION="20120122" COMMENT="XMLDB file for newsfeed module"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="local_s3coursedelete" COMMENT="AWS credentials">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="accesskey" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="secret_accesskey" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="region" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="version" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="bucketname" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>